{
  "name": "recalc-ts",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@creativebulma/bulma-divider": "1.1.0",
    "@creativebulma/bulma-tooltip": "1.2.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@tippyjs/react": "4.2.6",
    "@types/node": "20.8.3",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "adze": "1.9.1",
    "bulma": "0.9.4",
    "chart.js": "3.8.0",
    "chartjs-plugin-zoom": "1.2.1",
    "crypto-js": "4.2.0",
    "fast-cartesian": "8.0.0",
    "js-quantities": "1.8.0",
    "katex": "0.16.9",
    "lodash": "4.17.21",
    "lz-string": "1.5.0",
    "marked": "9.1.1",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-chartjs-2": "4.3.1",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-router-dom": "6.16.0",
    "react-table": "7.8.0",
    "react-use-promise": "0.5.0",
    "serialize-query-params": "2.0.2",
    "tinycolor2": "1.6.0",
    "tippy.js": "6.3.7",
    "ts-toolbelt": "9.6.0",
    "typescript": "5.2.2",
    "use-query-params": "2.2.1",
    "vitest": "0.34.6",
    "web-vitals": "3.5.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": ". ./.env && vitest",
    "--------------------------------": "------------------------",
    "dev:analyze": "source-map-explorer 'build/static/js/*.js' --html coverage/src_map_explorer.html",
    "dev:coverage": "yarn test --coverage",
    "dev:format": "npx prettier . --write && npx eslint src/ --fix",
    "dev:encrypt": "npx ts-node -O '{\"module\":\"commonjs\"}' scripts/encrypt.ts",
    "dev:logs": "npx ts-node -O '{\"module\":\"commonjs\"}' scripts/decodeLogs.ts",
    "dev:webp": "node scripts/webp.mjs",
    "dev:cypress": "yarn run cypress open",
    "dev:cypress-run": "yarn run cypress run",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@jedmao/location": "3.0.0",
    "@types/crypto-js": "4.1.3",
    "@types/google-spreadsheet": "3.2.2",
    "@types/js-quantities": "1.6.5",
    "@types/katex": "0.16.3",
    "@types/lodash": "4.14.199",
    "@types/lz-string": "1.5.0",
    "@types/marked": "6.0.0",
    "@types/react-helmet": "6.1.8",
    "@types/react-router-dom": "5.3.3",
    "@types/react-table": "7.7.17",
    "@types/tinycolor2": "1.4.5",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "@vitejs/plugin-react": "4.1.0",
    "@vitest/coverage-istanbul": "0.34.6",
    "cypress": "13.3.2",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "husky": "8.0.3",
    "imagemin": "8.0.1",
    "imagemin-webp": "8.0.0",
    "jsdom": "22.1.0",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "3.2.3",
    "sass": "1.69.5",
    "source-map-explorer": "2.5.3",
    "type-fest": "4.4.0",
    "vite": "4.5.0",
    "vite-plugin-svgr": "4.1.0",
    "vite-plugin-top-level-await": "1.3.1",
    "vite-tsconfig-paths": "4.2.1"
  }
}
